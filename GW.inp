    SORT_BASIS EXP
    POTENTIAL_FILE_NAME  GTH_POTENTIALS
    CHARGE 0
    &MGRID
      CUTOFF  500
      REL_CUTOFF  100
    &END MGRID
    &QS
      METHOD GPW
      EPS_DEFAULT 1.0E-40
      EPS_PGF_ORB 1.0E-40
    &END QS
    &SCF
      SCF_GUESS RESTART
      EPS_SCF 1.0E-9
      MAX_SCF 100
      &PRINT
        &RESTART ON
        &END
      &END
      ADDED_MOS -1
      &MIXING
          METHOD BROYDEN_MIXING
          ALPHA 0.1
          BETA 1.5
          NBROYDEN 8
      &END
    &END SCF
    &XC
      &XC_FUNCTIONAL LDA
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &PROPERTIES
    &BANDSTRUCTURE
      KPOINTS_DOS 2 2 1
      &KPOINTS_BANDSTRUCTURE
        SPECIAL_POINT Gamma 0.0   0.0   0.0
        SPECIAL_POINT M     0.5   0.0   0.0
        SPECIAL_POINT K     0.333 0.333 0.0
        SPECIAL_POINT Gamma 0.0   0.0   0.0
        NPOINTS 10
      &END
      &GW
        NUM_TIME_FREQ_POINTS 10
        MEMORY_PER_PROC               4
      &END
    &END
  &END PROPERTIES
  &SUBSYS
    &CELL
      ABC                 3.15 3.15 15.0
      ALPHA_BETA_GAMMA    90 90 120
        PERIODIC XY
        MULTIPLE_UNIT_CELL 10 10 1
    &END CELL
    &TOPOLOGY
      MULTIPLE_UNIT_CELL 10 10 1
    &END TOPOLOGY
    &KIND Mo
      BASIS_SET ORB    TZVP-MOLOPT-SR-GTH_robust_12
      BASIS_SET RI_AUX def2-SVP-RIFIT-mod-2
      POTENTIAL        GTH-PADE-q14
    &END KIND
    &KIND S
      BASIS_SET ORB    TZVP-MOLOPT-GTH_robust_9
      BASIS_SET RI_AUX def2-SVP-RIFIT-mod-1
      POTENTIAL        GTH-PADE-q6
    &END KIND
    &KIND W
      BASIS_SET ORB    TZVP-MOLOPT-SR-GTH_robust_1
      BASIS_SET RI_AUX def2-SVP-RIFIT-mod-3
      POTENTIAL        GTH-PADE-q14
    &END KIND
    &KIND Se
      BASIS_SET ORB    TZVP-MOLOPT-SR-GTH
      BASIS_SET RI_AUX def2-SVP-RIFIT-mod-3
      POTENTIAL        GTH-PADE-q6
    &END KIND
